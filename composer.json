{
    "name": "farzai/app-settings",
    "description": "PHP App settings",
    "keywords": [
        "farzai",
        "app-settings"
    ],
    "homepage": "https://github.com/farzai/app-settings-php",
    "license": "MIT",
    "authors": [
        {
            "name": "parsilver",
            "email": "parkorn@farzai.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Farzai\\AppSettings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Farzai\\AppSettings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
